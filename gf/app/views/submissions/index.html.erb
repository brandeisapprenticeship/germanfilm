<p id="notice"><%= notice %></p>
<h1>Class Submissions </h1>
<table class="table">
    <thead>
    <tr>
      <th>Student Name </th>
      <th>Submission </th>
      <th > Number Of Bookmarks</th>
      <th><%if current_user.admin? %> Grade Out of 10 <%end%></th>
    </tr>
     </thead>

  <tbody>
    <% @submissions.each do |submission| %>
      <tr>
        <td> <%= current_user.name %></td>
        <td><%= link_to submission.title, submission%></td> <!-- NEED TO CHANGE LINK -->
        <td><%= 'number of bookmarks here' %></td>
        <%if current_user.admin?%>
          <td><%='grade here'%></td>
        <%end%>
       <br>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
